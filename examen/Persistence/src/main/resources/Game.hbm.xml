<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.example.Game" table="games">
        <id name="id" type="java.lang.Integer" column="id">
            <generator class="increment"/>
        </id>
<!--        int userId, String duration, int points, String positionsChoosed, Boolean isOver-->
        <property name="userId" column="user_id" type="int"/>
        <property name="duration" column="duration" type="java.lang.String"/>
        <property name="points" column="points" type="int"/>
        <property name="positionsChoosed" column="positions_choosed" type="java.lang.String"/>
        <property name="isOver" column="is_over" type="boolean"/>

    </class>
</hibernate-mapping>